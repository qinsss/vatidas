<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.vatidas.entity.InOutStatistic" table="inOutStatisticView">
        <composite-id>
        	<key-property name="invoNum" type="integer"/>
        	<key-property name="money" type="big_decimal"/>
        	<key-property name="type" type="string"/>
        	<key-property name="taxMoney" type="big_decimal"/>
			<key-property name="yearMonth" type="date"/>
        </composite-id>
    </class>
</hibernate-mapping>
